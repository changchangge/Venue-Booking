/* pages/information/history/history.wxss */

.button-style {
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  padding: 0;
  margin: 0 30rpx 20rpx 0;
  float: right;
}

.loading {
  margin: 20rpx auto;
  display: flex;
  justify-content: center;
  height: 40rpx;
}

.loading text {
  color: #b2b2b2;
  line-height: 40rpx;
}

.loading-icon {
  padding-right: 10rpx;
  margin: auto 0;
  width: 32rpx;
  height: 32rpx;
}

.loading-content {
  color: #b2b2b2;
  margin: 30rpx;
  text-align: center;
}
